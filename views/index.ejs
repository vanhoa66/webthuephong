<% include header %>
    <!--  end hero section  -->
    <section class="search">
        <div class="wrapper">
            <h2>GHẾ VĂN PHÒNG</h2>
        </div>
    </section>

    <section class="listings">
        <div class="wrapper">
            <ul class="properties_list">
                <% rooms.forEach(function(room){ %>
                    <li>
                        <a href="#">
                            <img src="<%= room.image %>" alt="" title="" class="property_img" />
                        </a>
                        <span class="price">
                            <%= room.price %>
                        </span>
                        <div class="property_details">
                            <h1>
                                <a href="#">
                                    <%= room.name %>
                                </a>
                            </h1>
                            <h2>
                                <%- room.description %>
                            </h2>
                            <span class="property_size">
                                    <a href="/room/<%= room.id %>">
                                        <button type="button" class="btn btn-info">View Info</button>
                                    </a>
                                <a href="/room/<%= room.id %>/edit">
                                    <button type="button" class="btn btn-primary">Edit</button>
                                </a>
                                <form action="/room/<%= room.id %>?_method=DELETE" method="POST">
                                    <button type="submit" class="btn btn-danger">
                                        Delete
                                    </button>
                                </form>
                            </span>
                        </div>
                    </li>
                    <% }) %>
            </ul>
            <div class="more_listing">
                <a href="#" class="more_listing_btn">View More Listings</a>
            </div>
        </div>
    </section>
    <!--  end listing section  -->

    <% include footer %>